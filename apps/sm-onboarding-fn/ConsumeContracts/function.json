{
  "bindings": [
    {
      "type": "eventHubTrigger",
      "name": "rawContract",
      "direction": "in",
      "eventHubName": "%CONTRACTS_TOPIC_NAME%",
      "consumerGroup": "%CONTRACTS_TOPIC_CONSUMER_GROUP%",
      "connection": "CONTRACTS_CONSUMER_CONNECTION_STRING",
      "cardinality": "one"
    }
  ],
  "retry": {
    "strategy": "exponentialBackoff",
    "maxRetryCount": -1,
    "minimumInterval": "00:00:05",
    "maximumInterval": "00:05:00"
  },
  "scriptFile": "../dist/ConsumeContracts/index.js"
}
